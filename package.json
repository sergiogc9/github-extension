{
	"name": "fox-github-extension",
	"version": "0.1.3",
	"description": "Fox Github extension",
	"author": "Sergio GÃ³mez Cosgaya <sergiogcosgaya@gmail.com>",
	"keywords": [
		"chrome",
		"extension",
		"github"
	],
	"scripts": {
		"build:background": "NODE_ENV=production webpack --config webpack.config.js",
		"build:html": "npm run build --prefix src/html && cd src/html/build && cpy * /static/* ../../../dist/html --parents",
		"build": "npm-run-all build:*",
		"clean": "rm -rf dist && cpy manifest.json dist && cpy src/assets/* dist/assets",
		"prebuild": "npm run clean",
		"prettier": "prettier --write . --ignore-path .gitignore",
		"prettier:check": "prettier --check . --ignore-path .gitignore",
		"prewatch": "npm run clean",
		"watch:background": "NODE_ENV=development webpack --config webpack.config.js -w",
		"watch:html": "watch 'yarn build:html' ./src/html/src",
		"watch": "npm-run-all --parallel watch:*",
		"zip": "bestzip extension.zip dist/*"
	},
	"devDependencies": {
		"@sergiogc9/eslint-config": "1.0.7",
		"@types/chrome": "0.0.178",
		"@types/node": "17.0.18",
		"@typescript-eslint/eslint-plugin": "5.12.0",
		"awesome-typescript-loader": "5.2.1",
		"babel-eslint": "10.1.0",
		"bestzip": "2.2.0",
		"cpy-cli": "3.1.1",
		"css-loader": "3.5.3",
		"eslint": "8.9.0",
		"eslint-plugin-eslint-comments": "3.2.0",
		"eslint-plugin-import": "2.25.4",
		"eslint-plugin-prettier": "4.0.0",
		"mini-css-extract-plugin": "0.9.0",
		"node-sass": "^6",
		"npm-run-all": "4.1.5",
		"prettier": "2.5.1",
		"sass-loader": "^10",
		"typescript": "4.1.6",
		"watch": "^1.0.2",
		"webpack": "4.42.0",
		"webpack-cli": "3.3.11"
	},
	"dependencies": {
		"lodash": "4.17.21",
		"moment": "2.29.1"
	},
	"engines": {
		"node": ">=16",
		"npm": ">=8"
	}
}
